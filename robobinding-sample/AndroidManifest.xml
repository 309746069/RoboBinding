        <activity
            android:name="sample.robobinding.CursorBackedViewAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="sample.robobinding.ViewAlbumActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="sample.robobinding.ListViewAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="sample.robobinding.ListViewWithPredefinedViewsAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="sample.robobinding.SpinnerAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="sample.robobinding.SpinnerWithPredefinedViewsAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="sample.robobinding.DynamicItemLayoutListViewAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".DynamicDropdownLayoutSpinnerAlbumsActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>